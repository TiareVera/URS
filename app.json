{
  "expo": {
    "name": "MawatApp",
    "slug": "MawatApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo_blanco.jpeg",
    "userInterfaceStyle": "light",
    "scheme":"mawat",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo_sinfondo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/logo_sinfondo.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
