    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.5",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-web": "~0.19.13"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@types/react": "^18.3.18",
    "babel-plugin-react-native-web": "^0.19.13",
    "babel-preset-expo": "^12.0.4",
    "concurrently": "^9.1.1",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~8.0.1",
